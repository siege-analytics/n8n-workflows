    {
      "parameters": {
        "jsCode": "// Build fix requests for state drift\nconst driftData = $('Find Drift').first().json;\nconst drifts = driftData.drifts;\nconst fixes = [];\n\nfor (const drift of drifts) {\n  if (drift.fix === 'close_gh') {\n    // ClickUp is closed but GitHub is open — close the GH issue\n    fixes.push({\n      json: {\n        fixType: 'close_gh',\n        url: `https://api.github.com/repos/${drift.ghRepo}/issues/${drift.ghNumber}`,\n        body: { state: 'closed', state_reason: 'completed' },\n        description: drift.description\n      }\n    });\n  }\n  // 'flag_review' drifts are included in the summary but not auto-fixed\n}\n\nif (fixes.length === 0) {\n  return [];\n}\n\nreturn fixes;\n"
      },
