    {
      "parameters": {
        "jsCode": "// Check if there are more pages and fetch them\nconst firstPage = $('Fetch GH Issues Page 1').first().json;\nconst totalCount = firstPage.total_count || 0;\nconst firstPageItems = firstPage.items || [];\n\n// GitHub search API returns max 100 per page\n// If total_count > 100, we need more pages — but for now collect what we have\n// and note if we're missing items\nconst allIssues = firstPageItems;\n\n// Build a lookup: repo_short/number -> issue data\nconst ghLookup = {};\nfor (const issue of allIssues) {\n  // Extract repo short name from repository_url\n  const repoUrl = issue.repository_url || '';\n  const repoParts = repoUrl.split('/');\n  const repoOwner = repoParts[repoParts.length - 2];\n  const repoName = repoParts[repoParts.length - 1];\n\n  // Map to short names used in ClickUp task titles\n  let repoShort = repoName;\n  if (repoOwner === 'electinfo') {\n    repoShort = repoName; // enterprise, rundeck, ops, portainer\n  } else if (repoOwner === 'siege-analytics' && repoName === 'siege_utilities') {\n    repoShort = 'siege_utilities';\n  }\n\n  ghLookup[`${repoShort}#${issue.number}`] = {\n    number: issue.number,\n    state: issue.state,\n    title: issue.title,\n    repoShort: repoShort,\n    repoFull: `${repoOwner}/${repoName}`,\n    assignees: (issue.assignees || []).map(a => a.login),\n    labels: (issue.labels || []).map(l => l.name),\n    url: issue.html_url\n  };\n}\n\nreturn [{\n  json: {\n    ghLookup: ghLookup,\n    ghTotalCount: totalCount,\n    ghFetchedCount: allIssues.length,\n    truncated: totalCount > allIssues.length\n  }\n}];\n"
      },
